							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/give @s carrot",
						"/scoreboard players add @s score -5"
					]
				},
				"pcb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/give @s sugar_cane",
						"/scoreboard players add @s score -5"
					]
				},
				"pdb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/give @s potato",
						"/scoreboard players add @s score -5"
					]
				},
				"peb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/give @s beetroot_seeds",
						"/scoreboard players add @s score -5"
					]
				},
				"pfb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/give @s pumpkin_seeds",
						"/scoreboard players add @s score -10"
					]
				},
				"pgb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/give @s melon_seeds",
						"/scoreboard players add @s score -10"
					]
				},
				"phb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/give @s bamboo",
						"/scoreboard players add @s score -10"
					]
				},
				"pib": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/summon villager",
						"/scoreboard players add @s score -1000"
					]
