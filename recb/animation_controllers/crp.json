							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 兑换 村民(1000)",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:crpi"
					]
				},
				"pia": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 兑换 小麦种子(1)",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:crpa"
					]
				},
				"pab": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/give @s wheat_seeds",
						"/scoreboard players add @s score -1"
					]
				},
				"pbb": {
