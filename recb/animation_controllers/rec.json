{
	"format_version": "1.8.0",
	"animation_controllers": {
		"controller.animation.rec":{
			"states":{
				"base_jeta":{
					"transitions":[
						{"raa":"query.get_equipped_item_name=='reca' && query.is_using_item && !query.is_sneaking"},
						{"rba":"query.get_equipped_item_name=='recb' && query.is_using_item && !query.is_sneaking"},
						{"rca":"query.get_equipped_item_name=='recc' && query.is_using_item && !query.is_sneaking"},
						{"rda":"query.get_equipped_item_name=='recd' && query.is_using_item && !query.is_sneaking"},
						{"rea":"query.get_equipped_item_name=='rece' && query.is_using_item && !query.is_sneaking"},
						{"rfa":"query.get_equipped_item_name=='recf' && query.is_using_item && !query.is_sneaking"},
						{"rga":"query.get_equipped_item_name=='recg' && query.is_using_item && !query.is_sneaking"},
						{"rha":"query.get_equipped_item_name=='rech' && query.is_using_item && !query.is_sneaking"},
						{"ria":"query.get_equipped_item_name=='reci' && query.is_using_item && !query.is_sneaking"},
						{"rab":"query.get_equipped_item_name=='reca' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 50"},
						{"rbb":"query.get_equipped_item_name=='recb' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 10"},
						{"rcb":"query.get_equipped_item_name=='recc' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 20"},
						{"rdb":"query.get_equipped_item_name=='recd' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 100"},
						{"reb":"query.get_equipped_item_name=='rece' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 200"},
						{"rfb":"query.get_equipped_item_name=='recf' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 50"},
						{"rgb":"query.get_equipped_item_name=='recg' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 300"},
						{"rhb":"query.get_equipped_item_name=='rech' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 1000"},
						{"rib":"query.get_equipped_item_name=='reci' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 5000"}
					]						
				},
				"raa": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 兑换 橡木原木(10)",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:recb"
					]
				},
				"rba": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 兑换 煤炭(20)",
