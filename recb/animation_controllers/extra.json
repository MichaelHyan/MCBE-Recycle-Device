{
	"format_version": "1.8.0",
	"animation_controllers": {
		"controller.animation.extra":{
			"states":{
				"base_jeta":{
					"transitions":[
						{"jetpack_a":"!query.is_jumping && query.get_equipped_item_name=='jetpack'"},
						{"jetpack_b":"query.is_jumping && query.get_equipped_item_name=='jetpack'"}
					]
				},
				"jetpack_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/effect @s slow_falling 1 1 true",
						"/particle minecraft:balloon_gas_particle ^0.3 ^ ^",
						"/particle minecraft:balloon_gas_particle ^-0.3 ^ ^"
					]
				},
				"jetpack_b": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/effect @s levitation 1 5 true",
						"/particle minecraft:basic_flame_particle ^0.3 ^ ^",
						"/particle minecraft:basic_flame_particle ^-0.3 ^ ^",
						"/particle minecraft:balloon_gas_particle ^0.3 ^ ^",
						"/particle minecraft:balloon_gas_particle ^-0.3 ^ ^"
					]
				}
			}
		}
 	}
 }