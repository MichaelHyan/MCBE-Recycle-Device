{
	"format_version": "1.8.0",
	"animation_controllers": {
		"controller.animation.cash":{
			"states":{
				"base_jeta":{
					"transitions":[
						{"casha":"query.get_equipped_item_name=='ca' && query.is_using_item"},
						{"cashb":"query.get_equipped_item_name=='cb' && query.is_using_item"},
						{"cashc":"query.get_equipped_item_name=='cc' && query.is_using_item"},
						{"cashd":"query.get_equipped_item_name=='cd' && query.is_using_item"}
					]
				},
				"casha": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/scoreboard players add @s score 1"
					]
				},
				"cashb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/scoreboard players add @s score 10"
					]
				},
				"cashc": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/scoreboard players add @s score 100"
					]
				},
				"cashd": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/scoreboard players add @s score 1000"
					]
				}
			}
		}
 	}
 }