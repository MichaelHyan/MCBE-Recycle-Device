{
	"format_version": "1.8.0",
	"animation_controllers": {
		"controller.animation.red":{
			"states":{
				"base_jeta":{
					"transitions":[
						{"daa":"query.get_equipped_item_name=='reda' && query.is_using_item && !query.is_sneaking"},
						{"dba":"query.get_equipped_item_name=='redb' && query.is_using_item && !query.is_sneaking"},
						{"dca":"query.get_equipped_item_name=='redc' && query.is_using_item && !query.is_sneaking"},
						{"dda":"query.get_equipped_item_name=='redd' && query.is_using_item && !query.is_sneaking"},
						{"dea":"query.get_equipped_item_name=='rede' && query.is_using_item && !query.is_sneaking"},
						{"dfa":"query.get_equipped_item_name=='redf' && query.is_using_item && !query.is_sneaking"},
						{"dga":"query.get_equipped_item_name=='redg' && query.is_using_item && !query.is_sneaking"},
						{"dha":"query.get_equipped_item_name=='redh' && query.is_using_item && !query.is_sneaking"},
						{"dia":"query.get_equipped_item_name=='redi' && query.is_using_item && !query.is_sneaking"},
						{"dab":"query.get_equipped_item_name=='reda' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 5"},
						{"dbb":"query.get_equipped_item_name=='redb' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 20"},
						{"dcb":"query.get_equipped_item_name=='redc' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 20"},
						{"ddb":"query.get_equipped_item_name=='redd' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 20"},
						{"deb":"query.get_equipped_item_name=='rede' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 10"},
						{"dfb":"query.get_equipped_item_name=='redf' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 10"},
						{"dgb":"query.get_equipped_item_name=='redg' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 10"},
						{"dhb":"query.get_equipped_item_name=='redh' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 10"},
						{"dib":"query.get_equipped_item_name=='redi' && query.is_using_item && query.is_sneaking && query.scoreboard('score') >= 10"}
